{
  "name": "leya",
  "version": "0.0.1",
  "description": "Customize your browsing experience with tailored aesthetics and background sounds!",
  "permissions": ["tabs","activeTab","declarativeContent", "storage", "topSites"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_icon": "leya.png",
    "default_popup": "popup/popup.html"
  },
  "content_scripts": [
    {
        "matches": ["<all_urls>"],
        "css": ["app.css"],
        "js": ["app.js"]
    }],
        "web_accessible_resources": [
    "audio/cafe.mp3",
    "audio/rain.mp3",
    "audio/beach.mp3"
],
  "manifest_version": 2, 
  "chrome_url_overrides" : {
    "newtab": "/themes/newTab.html"
  }
}